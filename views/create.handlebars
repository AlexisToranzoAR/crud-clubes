<h1>Crear un equipo</h1>
<form action="/create" method="post" enctype="multipart/form-data">
    <ul style="list-style: none; padding: 0; margin: 0;">
        <li>
            <label>Logo: </label>
            <input type="file" name="logo" required>
        </li>
        <li>
            <label>Nombre: </label>
            <input type="text" name="name" placeholder="Arsenal FC" required>
        </li>
        <li>
            <label>Sobrenombre: </label>
            <input type="text" name="shortName" placeholder="Arsenal" required>
        </li>
        <li>
            <label>Pais: </label>
            <input type="text" name="area[name]" placeholder="England" required>
        </li>
        <li>
            <label>TLA: </label>
            <input type="text" name="tla" placeholder="ARS" required>
        </li>
        <li>
            <label>Dirección: </label>
            <input type="text" name="address" placeholder="75 Drayton Park London N5 1BU" required>
        </li>
        <li>
            <label>Telefono: </label>
            <input type="tel" name="phone" placeholder="+44 (020) 76195003" required>
        </li>
        <li>
            <label>Pagina web: </label>
            <input type="url" name="website" placeholder="http://www.arsenal.com" required>
        </li>
        <li>
            <label>Correo electronico: </label>
            <input type="email" name="email" placeholder="info@arsenal.co.uk" required>
        </li>
        <li>
            <label>Fundación: </label>
            <input type="number" name="founded" placeholder="1886" required>
        </li>
        <li>
            <label>Colores del club: </label>
            <input type="text" name="clubColors" placeholder="Red / White" required>
        </li>
        <li>
            <label>Estadio: </label>
            <input type="text" name="venue" placeholder="Emirates Stadium" required>
        </li>
        <li>
            <input type="submit" value="Subir">
        </li>
    </ul>
</form>